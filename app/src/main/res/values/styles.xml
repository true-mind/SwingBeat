<resources>

    <!-- Base application theme. -->
    <style name="AppThemeGreenBar" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppThemeTranslucent" parent="Theme.AppCompat.Dialog">
        <item name="android:windowBackground">@color/color_empty</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="commonDialogBtnTextStyle">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#fff</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="commonDialogBtnStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_marginLeft">30dp</item>
        <item name="android:layout_marginRight">30dp</item>
        <item name="android:layout_marginBottom">10dp</item>
    </style>


    <style name="btnBaseStyle1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="gateBtnStyle">
        <item name="android:layout_marginLeft">24dp</item>
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_height">100dp</item>
    </style>

    <style name="gateTextBase">
        <item name="android:layout_marginLeft">24dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="gateTextTop">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#000</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="gateTextBelow">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#000</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="settingSpinnerDescription">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#000</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <style name="settingSpinnerDescription2">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#666</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <style name="settingSpinnerStyle">
        <item name="android:background">@drawable/spinner</item>
        <item name="android:layout_width">400dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginLeft">30dp</item>
    </style>

    <style name="settingSpinnerBase">
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="rhythmLaneBase">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">58dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:weightSum">640</item>
    </style>

    <style name="rhythmLaneFrame">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">516</item>
    </style>

    <style name="rhythmBtnStyle">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">53</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="rhythmBtnTextStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#000</item>
        <item name="android:textSize">24sp</item>
    </style>

    <style name="whiteBackLayoutFullMargin20">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">#ffffff</item>
    </style>

    <style name="bottomBtnCommonBase">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_marginBottom">20dp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/s_btn_common_test</item>
    </style>

    <style name="bottomBtnCommonText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">#fff</item>
    </style>

    <style name="testTxtCommonTitle">
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">28sp</item>
        <item name="android:textColor">#000</item>
    </style>

    <style name="testDescriptionText1">
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#000</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="testDescriptionText2">
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#1f0e89</item>
        <item name="android:textSize">30sp</item>
    </style>

    <style name="testBtnColorStyle">
        <item name="android:layout_height">70dp</item>
        <item name="android:layout_width">70dp</item>
        <item name="android:layout_marginRight">10dp</item>
    </style>

    <style name="testScoreTextStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#000</item>
    </style>

    <style name="testResultTitleTextStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">#000</item>
    </style>

    <style name="testResultScoreTextStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">60sp</item>
        <item name="android:textColor">#000</item>
    </style>

    <style name="titlePartition4dp">
        <item name="android:layout_marginRight">40dp</item>
        <item name="android:layout_marginLeft">40dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">4dp</item>
        <item name="android:background">#000</item>
    </style>

    <style name="blackCenterTextStyle">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#000</item>
    </style>

    <style name="frameTextOnStart">
        <item name="android:layout_gravity">start|center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">#000</item>
    </style>

    <style name="frameTextOnEnd">
        <item name="android:layout_gravity">end|center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">24sp</item>
    </style>

    <style name="drumRdoBtnStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@null</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:textSize">20sp</item>
        <item name="android:gravity">center</item>
        <item name="android:button">@drawable/check_box</item>
    </style>

</resources>
